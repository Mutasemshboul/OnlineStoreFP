@model IEnumerable<OnlineStoreFP.Models.ContactusFp>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdmindashbordLayout.cshtml";
}


<div class="table-responsive-lg">
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table" id = "example">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subject)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Message)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Message)
            </td>
            <td>
                <a class="fa fa-edit" asp-action="Edit" asp-route-id="@item.Contactid"> </a> |
                <a class="fa fa-bars" asp-action="Details" asp-route-id="@item.Contactid"> </a> |
                <a class="fa fa-times" asp-action="Delete" asp-route-id="@item.Contactid"> </a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>
